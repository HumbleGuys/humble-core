{
    "name": "humble-guys/humble-core",
    "type": "library",
    "autoload": {
        "psr-4": {
            "HumbleCore\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Kim Larsson",
            "email": "kim@humbleguys.se"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/container": "^9.19",
        "illuminate/view": "^9.19",
        "illuminate/support": "^9.19",
        "illuminate/config": "^9.19",
        "illuminate/filesystem": "^9.19",
        "illuminate/contracts": "^9.19",
        "illuminate/http": "^9.19",
        "vlucas/phpdotenv": "^5.4",
        "symfony/var-dumper": "^6.1",
        "illuminate/events": "^9.19"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "laravel/pint": "^0.2.3",
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse",
        "lint": "./vendor/bin/pint -v",
        "test": "./vendor/bin/pest"
    }
}
